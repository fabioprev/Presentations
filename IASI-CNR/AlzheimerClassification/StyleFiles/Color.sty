%%%%%%%% Colors
\setbeamercolor{example text}{fg=secondarydarkblue}

\setbeamercolor{structure}{fg=institute}

\setbeamercolor{palette primary}{fg=instituteNEW,bg=instituteNEW}
\setbeamercolor{palette secondary}{fg=institute,bg=institutecomplementary}
\setbeamercolor{palette tertiary}{fg=secondarylightazul,bg=institute}
\setbeamercolor{palette quaternary}{bg=secondarydarkazul,fg=institutedark}

\setbeamercolor{titlelike}{fg=secondarywhite}
\setbeamercolor{title in head/foot}{parent=palette tertiary}
\setbeamercolor{subtitle}{parent=title,fg=institutedark}
\setbeamercolor{author in head/foot}{parent=palette primary}
\setbeamercolor{institute in head/foot}{parent=palette tertiary}
\setbeamercolor{date in head/foot}{parent=palette secondary}
\setbeamercolor{frametitle}{fg=institutedark}
\setbeamercolor{framesubtitle}{parent=frametitle,fg=institutedark}
\setbeamercolor{navigation symbols}{use=palette tertiary,fg=palette tertiary.fg!40!bg}
\setbeamercolor{navigation symbols dimmed}{use=palette tertiary,fg=palette tertiary.fg!40!bg}
\setbeamercolor{block body}{parent=normal text,use=block title,bg=secondarylightazul}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=secondarylightazul}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=secondarylightazul}
\setbeamercolor{block title}{use=structure,fg=institutelight,bg=secondarydarkblue}
\setbeamercolor{block title alerted}{use=alerted text,bg=secondarydarkorange,fg=institutelight}
\setbeamercolor{block title example}{use=example text,bg=secondarylightgreen}
\setbeamercolor{bibliography entry location}{use=structure,fg=secondarydarkazul}
\setbeamercolor{separation line}{}
\setbeamercolor{fine separation line}{}
\mode
<all>