%%%%%%%%%%%%%%%%%%%%%% Sapienza palette
\definecolor{institute}{RGB}{0,69,134}
\definecolor{instituteNEW}{RGB}{0,0,0}
\definecolor{institutecomplementary}{RGB}{0,69,134}
\definecolor{institutedark}{RGB}{0,0,0}
\definecolor{institutelight}{RGB}{255,255,255}
\definecolor{secondarydarkgreen}{RGB}{112,164,137}
\definecolor{secondarylightgreen}{RGB}{170,201,182}
\definecolor{secondarydarkyellow}{RGB}{199,153,0}
\definecolor{secondarylightyellow}{RGB}{215,169,0}
\definecolor{secondarydarkorange}{RGB}{197,76,0}
\definecolor{secondarylightorange}{RGB}{246,146,64}
\definecolor{secondarydarkblue}{RGB}{0,103,120}
\definecolor{secondarylightblue}{RGB}{0,179,190}
\definecolor{secondarydarkgray}{RGB}{170,163,142}
\definecolor{secondarylightgray}{RGB}{215,211,199}
\definecolor{secondarydarkazul}{RGB}{22,77,102}
\definecolor{secondarylightazul}{RGB}{211,222,228}
\definecolor{secondarywhite}{RGB}{255,255,255}

\definecolor{sapienzacolor1}{named}{secondarydarkgreen}
\definecolor{sapienzacolor2}{named}{secondarydarkyellow}
\definecolor{sapienzacolor3}{named}{secondarydarkorange}
\definecolor{sapienzacolor4}{named}{secondarydarkblue}
\definecolor{sapienzacolor5}{named}{secondarydarkgray}
