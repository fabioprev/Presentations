\mode<presentation>

\setbeamercolor*{navigation symbols color}{fg=secondarydarkazul,bg=title in head/foot.bg}

\setbeamertemplate{navigation symbols}{}

\defbeamertemplate*{footline}{dis theme}
{
		
	\hspace{.13\paperwidth}
	\begin{beamercolorbox}[wd=.87\textwidth,ht=.12\paperheight]{title in head/foot}
		\vspace{.08\paperheight}
		\hspace{.01\paperwidth}
		\insertshorttitle\hfill
		\insertshortdate\hfill
		\insertframenumber{} / \inserttotalframenumber\hspace*{2em}
	\end{beamercolorbox}
	\vspace{-.08\paperheight}

	\begin{beamercolorbox}[wd=\paperwidth,ht=.08\paperheight,right]{navigation symbols color}
		\insertslidenavigationsymbol
		\insertframenavigationsymbol
		\insertbackfindforwardnavigationsymbol
		\hspace*{1em}
		\vspace*{1em}
	\end{beamercolorbox}
}

\setbeamersize{text margin left=1em,text margin right=1em}

\mode
<all>